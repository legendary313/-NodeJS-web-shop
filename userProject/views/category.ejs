    <script type='text/javascript'	> 
        function filterFunction() {
        var nikecheck = document.getElementById("nike").checked;
        var adidascheck = document.getElementById("adidas").checked;
        var vanscheck = document.getElementById("vans").checked;
        var bitischeck = document.getElementById("bitis").checked;
        var sneakercheck = document.getElementById("sneaker").checked;
		var blackcheck = document.getElementById("black").checked;
        var browncheck = document.getElementById("brown").checked;
        var greycheck = document.getElementById("grey").checked;
        var redcheck = document.getElementById("red").checked;

		var filter, ul, li;
		var datas = <%-JSON.stringify(data)%>;
		console.log(datas[0].nhanhieu);
    ul = document.getElementById("myUL");
    li = ul.getElementsByTagName("li");
      // Nếu filter không có giá trị thị ẩn phần kết quare\
    if (nikecheck===true) {		
		datas.forEach(element => {
			if(element.nhanhieu=="NIKE")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
			
    }
    if (adidascheck===true) {
        datas.forEach(element => {
			if(element.nhanhieu=="ADIDAS")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
    }
    if (vanscheck===true) {
        datas.forEach(element => {
			if(element.nhanhieu=="VANS")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
    }
    if (bitischeck===true) {
        datas.forEach(element => {
			if(element.nhanhieu=="BITIS")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
    }
    if (sneakercheck===true) {
        datas.forEach(element => {
			if(element.nhanhieu=="SNEAKER")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
	}

	if (blackcheck===true) {
		if(nikecheck===true || adidascheck === true || vanscheck === true || sneakercheck === true || bitischeck === true)
		{
			datas.forEach(element => {
			if(element.mau !="DEN")
			document.getElementById(element.magiay).style.display="none";
		});
		}
		else{
			datas.forEach(element => {
			if(element.mau=="DEN")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
		}
        
	}

	if (browncheck===true) {
        if(nikecheck===true || adidascheck === true || vanscheck === true || sneakercheck === true || bitischeck === true)
		{
			datas.forEach(element => {
			if(element.mau !="NAU")
			document.getElementById(element.magiay).style.display="none";
		});
		}
		else{
			datas.forEach(element => {
			if(element.mau=="NAU")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
		}
	}

	if (greycheck===true) {
		if(nikecheck===true || adidascheck === true || vanscheck === true || sneakercheck === true || bitischeck === true)
		{
			datas.forEach(element => {
			if(element.mau !="XAM")
			document.getElementById(element.magiay).style.display="none";
		});
		}
		else{
			datas.forEach(element => {
			if(element.mau=="XAM")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
		}
	}

	if (redcheck===true) {
        datas.forEach(element => {
			if(element.mau=="XAM")
        	document.getElementById(element.magiay).style.display="block";
        	else
			document.getElementById(element.magiay).style.display="none";
		});
	}

	
    }
    </script>
